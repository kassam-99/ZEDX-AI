{
  "QSS_LINES": [
    "QWidget {",
    "  font-family: Segoe UI, Noto Sans, Ubuntu, sans-serif;",
    "  font-size: 13px;",
    "  color: #C9D1D9;",
    "  selection-background-color: rgba(56,139,253,0.35);",
    "  selection-color: #F0F6FC;",
    "}",
    "QLineEdit::placeholder { color: #8B949E; }",

    "#MainWindow {",
    "  background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #0D1117, stop:0.55 #0A0F15, stop:1 #010409);",
    "}",

    "#Sidebar {",
    "  background: rgba(22, 27, 34, 0.94);",
    "  border: 1px solid #30363D;",
    "  border-radius: 16px;",
    "  min-width: 320px;",
    "}",

    "#MainPanel {",
    "  background: rgba(22, 27, 34, 0.90);",
    "  border: 1px solid #30363D;",
    "  border-radius: 16px;",
    "}",

    "#BtnNewChat {",
    "  background: #1F6FEB;",
    "  color: #F0F6FC;",
    "  border: 1px solid #388BFD;",
    "  border-radius: 14px;",
    "  padding: 12px 14px;",
    "  font-weight: 800;",
    "}",
    "#BtnNewChat:hover { background: #388BFD; }",
    "#BtnNewChat:pressed { background: #1158C7; }",

    "#BtnPersona, #BtnClear, #BtnUpload, #BtnMonitor {",
    "  background: #21262D;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "  padding: 9px 14px;",
    "  font-weight: 700;",
    "}",
    "#BtnPersona:hover, #BtnClear:hover, #BtnUpload:hover, #BtnMonitor:hover {",
    "  background: #30363D;",
    "  border: 1px solid #6E7681;",
    "}",
    "#BtnUpload:disabled { color: rgba(201,209,217,0.45); }",

    "#SearchBox, #InputBox, #ModalText {",
    "  background: #0D1117;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "  padding: 10px 12px;",
    "}",
    "#SearchBox:focus, #InputBox:focus, #ModalText:focus {",
    "  border: 1px solid #388BFD;",
    "}",

    "#ChatList, #FileList {",
    "  background: transparent;",
    "  border: none;",
    "}",
    "#ChatList::item, #FileList::item {",
    "  background: #161B22;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "  padding: 10px 12px;",
    "  margin: 5px;",
    "}",
    "#ChatList::item:hover, #FileList::item:hover {",
    "  border: 1px solid #6E7681;",
    "}",
    "#ChatList::item:selected, #FileList::item:selected {",
    "  background: rgba(31, 111, 235, 0.22);",
    "  border: 1px solid #58A6FF;",
    "  color: #F0F6FC;",
    "}",
    "#ChatList QLineEdit {",
    "  background: #0D1117;",
    "  color: #C9D1D9;",
    "  border: 1px solid #388BFD;",
    "  border-radius: 8px;",
    "  padding: 2px 6px;",
    "}",

    "QMenu {",
    "  background: #161B22;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "  padding: 6px;",
    "}",
    "QMenu::item {",
    "  padding: 8px 14px;",
    "  border-radius: 8px;",
    "  margin: 2px 4px;",
    "}",
    "QMenu::item:selected {",
    "  background: rgba(31,111,235,0.26);",
    "  color: #F0F6FC;",
    "}",
    "QMenu::separator {",
    "  height: 1px;",
    "  background: #30363D;",
    "  margin: 6px 10px;",
    "}",

    "#ScrollArea {",
    "  background: transparent;",
    "  border: none;",
    "}",
    "#MessagesViewport {",
    "  background: rgba(13, 17, 23, 0.70);",
    "  border: 1px solid #30363D;",
    "  border-radius: 16px;",
    "}",

    "#BubbleUser {",
    "  background: #1F6FEB;",
    "  color: #F0F6FC;",
    "  border: 1px solid #388BFD;",
    "  border-radius: 16px;",
    "  padding: 10px 12px;",
    "}",
    "#BubbleAssistant {",
    "  background: #161B22;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 16px;",
    "  padding: 10px 12px;",
    "}",

    "#CodePanel {",
    "  background: #0D1117;",
    "  border: 1px solid #30363D;",
    "  border-radius: 14px;",
    "}",
    "#BtnCopy {",
    "  background: #21262D;",
    "  border: 1px solid #30363D;",
    "  color: #C9D1D9;",
    "  border-radius: 10px;",
    "  padding: 6px 10px;",
    "  font-weight: 700;",
    "}",
    "#BtnCopy:hover { background: #30363D; }",

    "#BtnSend {",
    "  background: #1F6FEB;",
    "  color: #F0F6FC;",
    "  border: 1px solid #388BFD;",
    "  border-radius: 14px;",
    "  padding: 10px 16px;",
    "  font-weight: 900;",
    "}",
    "#BtnSend:hover { background: #388BFD; }",
    "#BtnSend:pressed { background: #1158C7; }",
    "#BtnSend:disabled {",
    "  background: rgba(33,38,45,0.92);",
    "  color: rgba(201,209,217,0.45);",
    "  border: 1px solid #30363D;",
    "}",

    "#Modal {",
    "  background: #161B22;",
    "  border: 1px solid #30363D;",
    "  border-radius: 16px;",
    "}",
    "#ModalTitle {",
    "  color: #F0F6FC;",
    "  font-size: 15px;",
    "  font-weight: 900;",
    "}",
    "#ModalSub {",
    "  color: #8B949E;",
    "  font-weight: 600;",
    "}",
    "#BtnModalSave {",
    "  background: #1F6FEB;",
    "  color: #F0F6FC;",
    "  border: 1px solid #388BFD;",
    "  border-radius: 12px;",
    "  padding: 10px 14px;",
    "  font-weight: 900;",
    "}",
    "#BtnModalSave:hover { background: #388BFD; }",
    "#BtnModalDefault {",
    "  background: #21262D;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "  padding: 10px 14px;",
    "  font-weight: 800;",
    "}",
    "#MonitorCard {",
    "  background: #0D1117;",
    "  border: 1px solid #30363D;",
    "  border-radius: 12px;",
    "}",
    "#MonitorTitle {",
    "  color: #8B949E;",
    "  font-weight: 800;",
    "}",
    "#MonitorValue {",
    "  color: #F0F6FC;",
    "  font-weight: 900;",
    "  font-size: 14px;",
    "}",
    "#MonitorText {",
    "  background: #0D1117;",
    "  color: #C9D1D9;",
    "  border: 1px solid #30363D;",
    "  border-radius: 10px;",
    "  padding: 8px;",
    "}",
    "#MonitorBar {",
    "  background: #161B22;",
    "  border: 1px solid #30363D;",
    "  border-radius: 6px;",
    "  min-height: 10px;",
    "}",
    "#MonitorBar::chunk {",
    "  background: #1F6FEB;",
    "  border-radius: 6px;",
    "}",

    "QScrollBar:vertical {",
    "  background: #0D1117;",
    "  width: 11px;",
    "  margin: 4px 2px 4px 2px;",
    "  border-radius: 5px;",
    "}",
    "QScrollBar::handle:vertical {",
    "  background: #6E7681;",
    "  min-height: 30px;",
    "  border-radius: 5px;",
    "}",
    "QScrollBar::handle:vertical:hover { background: #8B949E; }",
    "QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {",
    "  border: none;",
    "  background: none;",
    "  height: 0px;",
    "}",

    "#StatusBar {",
    "  color: #8B949E;",
    "  font-weight: 700;",
    "}"
  ]
}
